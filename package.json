{
  "name": "dashboard",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "seed": "echo \"#################################\nNode version should be 22.9 or above\n#################################\"; node --experimental-sqlite seeder/index.js",
    "test:integration": "playwright test",
    "test:unit": "vitest"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.0.1",
    "@fontsource/fira-mono": "^5.0.0",
    "@fontsource/geist-sans": "^5.0.3",
    "@neoconfetti/svelte": "^2.0.0",
    "@playwright/test": "^1.28.1",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/adapter-cloudflare-workers": "2.5.4",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "@sveltejs/vite-plugin-svelte-inspector": "2",
    "@svelteuidev/composables": "^0.15.7",
    "@svelteuidev/core": "^0.15.7",
    "@svelteuidev/dates": "^0.15.7",
    "@svelteuidev/motion": "^0.15.7",
    "@svelteuidev/preprocessors": "^0.15.7",
    "@svelteuidev/prism": "^0.15.7",
    "@types/eslint": "^9.6.0",
    "dayjs": "^1.11.13",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.36.0",
    "globals": "^15.0.0",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "svelte": "^4.2.19",
    "svelte-check": "^3.6.0",
    "svelte-icons": "^2.1.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.0.0",
    "vite": "^5.0.3",
    "vitest": "^2.0.0"
  },
  "type": "module",
  "dependencies": {
    "@auth/core": "^0.34.2",
    "@auth/sveltekit": "^1.4.2"
  }
}
